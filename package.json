{
  "name": "marked-toc-extension",
  "version": "3.2.0",
  "description": "Table of Contents extension for marked",
  "main": "lib/marked-toc-extension.js",
  "module": "lib/marked-toc-extension.esm.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf lib",
    "lint": "eslint .",
    "test": "jest",
    "check-type": "tsc --noEmit",
    "prepack": "npm run clean && npm run build",
    "prepare": "husky install"
  },
  "keywords": [
    "markdown",
    "extension",
    "toc",
    "table of contents"
  ],
  "author": "Huaichao Wang(huaichao.wang@qq.com)",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "rollup": "^3.14.0",
    "rollup-plugin-ts": "^3.2.0",
    "semantic-release": "^21.1.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "marked": "^14.1.4"
  },
  "dependencies": {
    "github-slugger": "^2.0.0"
  }
}
